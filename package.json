{
  "name": "audiotoolheadless",
  "version": "0.0.1",
  "description": "Headless Audio Player",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist && tsc --emitDeclarationOnly",
    "watch": "swc src -d dist -w --strip-leading-paths",
    "start": "node --enable-source-maps dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "pnpm typecheck && pnpm watch",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "check": "biome check --write ./src",
    "test": "echo \"No tests created\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n/a.git"
  },
  "keywords": [
    "audio",
    "headless",
    "audio"
  ],
  "author": "S T",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/n/a/issues"
  },
  "homepage": "https://github.com/n/a#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.7.36",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
